# Example script
# Copyright (C) 2007 OpenWrt.org

USE_PROCD=1
START=99
STOP=15

PROG="/usr/sbin/prince"
CONF="/etc/prince.conf"

start_service() {
        procd_open_instance
        procd_set_param command $PROG $CONF
        procd_set_param stdout 1
        procd_set_param stderr 1
        procd_set_param user nobody
        procd_close_instance
        echo "Prince started"
}
